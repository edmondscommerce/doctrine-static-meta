{
  "name": "edmondscommerce/doctrine-static-meta",
  "license": "MIT",
  "type": "project",
  "description": "An implementation of Doctrine using the PHP Static Meta Data driver and no annotations",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\": [
        "tests/"
      ],
      "My\\Test\\Project\\": [
        "example/project/src"
      ]
    }
  },
  "require": {
    "doctrine/orm": "~2",
    "php": ">=7.1",
    "symfony/console": "^4.0",
    "symfony/filesystem": "^3.3",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "gossi/php-code-generator": "^0.5.0",
    "symfony/validator": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "fzaninotto/faker": "dev-dsm-patches@dev",
    "edmondscommerce/phpqa": "dev-magento2-merge-master@dev",
    "overtrue/phplint": "^1.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/edmondscommerce/Faker.git"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "edmondscommerce/*": "source",
      "fzaninotto/faker": "source",
      "*": "dist"
    },
    "optimize-autoloader": true,
    "github-protocols": [
      "https"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
