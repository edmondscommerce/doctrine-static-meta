{
  "name": "edmondscommerce/doctrine-static-meta",
  "license": "MIT",
  "type": "project",
  "description": "An implementation of Doctrine using the PHP Static Meta Data driver and no annotations",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\Tests\\": [
        "tests"
      ],
      "Symfony\\Component\\Validator\\": [
        "vendor/symfony/validator/"
      ]
    }
  },
  "require": {
    "doctrine/orm": "~2",
    "doctrine/data-fixtures": "^1.3",
    "php": ">=7.2",
    "symfony/console": "^4.0",
    "symfony/filesystem": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/config": "^4.0",
    "gossi/php-code-generator": "^0.5.0",
    "symfony/validator": "^4.0",
    "moneyphp/money": "^3.1",
    "symfony/intl": "^4.1",
    "edmondscommerce/typesafe-functions": "dev-master@dev",
    "ramsey/uuid-doctrine": "^1.5",
    "ext-json": "*",
    "ext-mysqli": "*",
    "doctrine/migrations": "^1.8"
  },
  "require-dev": {
    "fzaninotto/faker": "v1.8.0",
    "edmondscommerce/phpqa": "^1.2",
    "overtrue/phplint": "^1.0",
    "roave/security-advisories": "dev-master@dev",
    "doctrine/data-fixtures": "^1.3"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "edmondscommerce/*": "source",
      "fzaninotto/faker": "source",
      "*": "dist"
    },
    "optimize-autoloader": true,
    "github-protocols": [
      "https"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
