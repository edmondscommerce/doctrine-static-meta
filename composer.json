{
  "name": "edmondscommerce/doctrine-static-meta",
  "license": "MIT",
  "type": "project",
  "description": "An implementation of Doctrine using the PHP Static Meta Data driver and no annotations",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\": [
        "tests/"
      ],
      "My\\Test\\Project\\": [
        "example/project/src"
      ]
    }
  },
  "require": {
    "doctrine/orm": "~2",
    "php": ">=7.1",
    "symfony/console": "^3.3",
    "symfony/filesystem": "^3.3",
    "gossi/php-code-generator": "^0.5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "fzaninotto/faker": "^1.7",
    "edmondscommerce/phpqa": "dev-master"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "edmondscommerce*": "source",
      "*": "dist"
    },
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
