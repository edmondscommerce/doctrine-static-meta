{
  "name": "edmondscommerce/doctrine-static-meta",
  "license": "MIT",
  "type": "project",
  "description": "An implementation of Doctrine using the PHP Static Meta Data driver and no annotations",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EdmondsCommerce\\DoctrineStaticMeta\\Tests\\": [
        "tests"
      ],
      "Symfony\\Component\\Validator\\": [
        "vendor/symfony/validator/"
      ]
    }
  },
  "require": {
    "php": "^8.0",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "doctrine/orm": "^2.9.3",
    "doctrine/dbal": "^2.13.2",
    "doctrine/migrations": "^3.1.4",
    "doctrine/data-fixtures": "^1.5",
    "symfony/console": "^5.3.2",
    "symfony/filesystem": "^5.3.0",
    "symfony/dependency-injection": "^5.3.2",
    "symfony/intl": "^5.3.0",
    "symfony/config": "^5.3.2",
    "symfony/validator": "^5.3.2",
    "moneyphp/money": "^3.3.1",
    "edmondscommerce/typesafe-functions": "dev-PHP8@dev",
    "ramsey/uuid-doctrine": "^1.6",
    "symfony/cache": "^5.3"
  },
  "repositories": [
    {
      "type": "vcs",
      "notes": "This is a fork of a fork!, forked from cristianoc72 which is a fork of the original gossi ",
      "url": "https://github.com/LongTermSupport/php-code-generator"
    },
    {
      "type": "vcs",
      "notes": "This is a fork of overtrue",
      "url": "https://github.com/LongTermSupport/phplint"
    }
  ],
  "require-dev": {
    "gossi/php-code-generator": "1.0.x-dev",
    "fakerphp/faker": "^v1.14.1",
    "edmondscommerce/phpqa": "dev-php8@dev",
    "phpmd/phpmd": "dev-master@dev",
    "roave/security-advisories": "dev-master@dev",
    "doctrine/data-fixtures": "^1.5.0",
    "overtrue/phplint": "dev-BugFix"
  },
  "suggest": {
    "sebastian/diff": "required for overrides commands"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "edmondscommerce/*": "source",
      "gossi/php-code-generator": "source",
      "overtrue/phplint": "source",
      "*": "dist"
    },
    "optimize-autoloader": true,
    "github-protocols": [
      "https"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
