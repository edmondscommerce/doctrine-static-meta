{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.1.3",
    "ext-iconv": "*",
    "edmondscommerce/doctrine-static-meta": "dev-master@dev",
    "edmondscommerce/dsm-symfony": "dev-master",
    "symfony/console": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/orm-pack": "^1.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "symfony/dotenv": "^4.0",
    "edmondscommerce/phpqa": "dev-master@dev",
    "fzaninotto/faker": "dev-dsm-patches@dev"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/edmondscommerce/Faker.git"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true,
    "bin-dir": "bin",
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "My\\Test\\Project\\": [
        "src/"
      ],
      "App\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "My\\Test\\Project\\": [
        "tests/"
      ],
      "App\\Tests\\": [
        "tests/"
      ]
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01CC0EFEC0NE9ANF3KWEQ2R9SK",
      "allow-contrib": false
    }
  }
}
